{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      formData: {\n        name: '',\n        checkin: 1\n      },\n      showDialog: false,\n      checkedIn: false\n    };\n  },\n  mounted() {\n    this.formData.name = localStorage.getItem('username');\n    console.log(this.formData.name);\n  },\n  methods: {\n    showConfirmDialog() {\n      this.showDialog = true;\n    },\n    hideConfirmDialog() {\n      this.showDialog = false;\n    },\n    checkIn() {\n      // 发送签到请求到后端\n      axios.post('/api/sign', this.formData).then(response => {\n        if (response.data.msg == 'success') {\n          // 签到成功\n          this.checkedIn = true;\n          this.showDialog = false;\n        } else if (response.data.error_num == 3) {\n          alert(\"签到失败,还没有选择体检项目，请先去选择!\");\n        } else {\n          alert(\"签到失败，还没有预约，请先去预约体检！\");\n        }\n      }).catch(error => {\n        // 处理请求错误\n        console.error(error);\n        alert(\"发生异常！\");\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA8BE,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,IAAI,EAAC,EAAE;QACPC,OAAO,EAAC;MACV,CAAC;MACDC,UAAU,EAAE,KAAK;MACjBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACV,IAAI,CAACL,QAAQ,CAACC,IAAG,GAAIK,YAAY,CAACC,OAAO,CAAC,UAAU;IACpDC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,QAAQ,CAACC,IAAI;EAC9B,CAAC;EACDS,OAAO,EAAE;IACPC,iBAAiB,GAAG;MAClB,IAAI,CAACR,UAAS,GAAI,IAAI;IACxB,CAAC;IACDS,iBAAiB,GAAG;MAClB,IAAI,CAACT,UAAS,GAAI,KAAK;IACzB,CAAC;IACDU,OAAO,GAAG;MACR;MACAf,KAAK,CAACgB,IAAI,CAAC,WAAW,EAAE,IAAI,CAACd,QAAQ,EAClCe,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAIA,QAAQ,CAACjB,IAAI,CAACkB,GAAG,IAAE,SAAS,EAAE;UAChC;UACA,IAAI,CAACb,SAAQ,GAAI,IAAI;UACrB,IAAI,CAACD,UAAS,GAAI,KAAK;QACzB,OAAO,IAAGa,QAAQ,CAACjB,IAAI,CAACmB,SAAS,IAAE,CAAC,EAAC;UACnCC,KAAK,CAAC,uBAAuB;QAC/B,OAAO;UACLA,KAAK,CAAC,qBAAqB;QAC7B;MACF,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChB;QACAb,OAAO,CAACa,KAAK,CAACA,KAAK;QACnBF,KAAK,CAAC,OAAO;MACf,CAAC;IACL;EACF